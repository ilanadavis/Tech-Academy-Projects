TextWindow.Write("Enter a number between 1-10:")
Number_1=TextWindow.ReadNumber()
TextWindow.Write("Enter a different number between 1-10:")
Number_2=TextWindow.ReadNumber()
TextWindow.Write("Which number is smaller? ")
Program.Delay(1000)
If Number_1<Number_2 Then
  TextWindow.Write(Number_1+ " is smaller!")
Else
  TextWindow.Write(Number_2+ " is smaller! ")
EndIf


